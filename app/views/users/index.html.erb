<div class = "container" >

    <% @users.each do |user| %>
      <div class="pro">
      <% if user.avatar.url %>
      <%= image_tag user.avatar.url %>
      <br>
      <br>
      <p>First Name: <%= user.fname%></p>
      <p>Last Name: <%= user.lname %></p>
      <p>Username: <strong><%=link_to user.username, user_path(user) %></strong></p>
      </div>
      <%end%>
    <%end%>

</div>
